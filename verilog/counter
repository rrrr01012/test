#! /usr/local/iverilog/bin/vvp
:ivl_version "11.0 (devel)" "(s20150603-642-g3bdb50da)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "vhdl_textio";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000008e8d40 .scope module, "main" "main" 2 8;
 .timescale 0 0;
v00000000008ea630_0 .var "clock", 0 0;
v00000000008e68b0_0 .net "i", 7 0, v00000000008ea4f0_0;  1 drivers
v00000000008e6950_0 .var "reset", 0 0;
S_00000000008ea360 .scope module, "c0" "counter" 2 11, 2 1 0, S_00000000008e8d40;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "reset";
    .port_info 1 /INPUT 1 "clock";
    .port_info 2 /OUTPUT 8 "count";
v00000000008e8ed0_0 .net "clock", 0 0, v00000000008ea630_0;  1 drivers
v00000000008ea4f0_0 .var "count", 7 0;
v00000000008ea590_0 .net "reset", 0 0, v00000000008e6950_0;  1 drivers
E_00000000008ec240 .event posedge, v00000000008e8ed0_0;
E_00000000008ec280 .event edge, v00000000008ea590_0;
    .scope S_00000000008ea360;
T_0 ;
    %wait E_00000000008ec280;
    %load/vec4 v00000000008ea590_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v00000000008ea4f0_0, 0, 8;
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_00000000008ea360;
T_1 ;
    %wait E_00000000008ec240;
    %load/vec4 v00000000008ea4f0_0;
    %addi 1, 0, 8;
    %store/vec4 v00000000008ea4f0_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_00000000008e8d40;
T_2 ;
    %vpi_call 2 13 "$display", "%4dns:reset=%d clock=%d i=%d", $stime, v00000000008e6950_0, v00000000008ea630_0, v00000000008e68b0_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000008e6950_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000008ea630_0, 0, 1;
    %vpi_call 2 15 "$display", "%4dns: reset=%d clock=%d i=%d", $stime, v00000000008e6950_0, v00000000008ea630_0, v00000000008e68b0_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000008e6950_0, 0, 1;
    %vpi_call 2 17 "$display", "%4dns: reset=%d clock=%d i=%d", $stime, v00000000008e6950_0, v00000000008ea630_0, v00000000008e68b0_0 {0 0 0};
    %delay 500, 0; 
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_00000000008e8d40;
T_3 ;
    %delay 40, 0;
    %load/vec4 v00000000008ea630_0;
    %inv;
    %store/vec4 v00000000008ea630_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 23 "$display", "%4dns: reset=%d clock=%d i=%d", $stime, v00000000008e6950_0, v00000000008ea630_0, v00000000008e68b0_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
